<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title></title>
  <link rel="stylesheet" href="">
</head>
<body>
  <header>
    <h1>Authors</h1>
    <nav>
      <ul>
        <li>
          <a href="/">Home</a>
        </li>
        <li>
          <a href="/authors/new">Create a new Author</a>
        </li>
      </ul>
    </nav>
  </header>

  <main>
    <h2>List of Authors</h2>
    <ul>
      <% authors.forEach((author) => { %>
        <li>
          <a href="/authors/<%= author._id%>"><%= author.name %></a>
        </li>
      <%})%>
    </ul>
  </main>

</body>
</html>
